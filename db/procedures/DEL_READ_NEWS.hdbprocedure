PROCEDURE "CART_REQUEST_TOOL"."xscartrequesttool.db.procedures::DEL_READ_NEWS" (
    IN in_news_id BIGINT,
    OUT out_result BIGINT
 )
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "CART_REQUEST_TOOL"
	AS
BEGIN

    DELETE FROM "CART_REQUEST_TOOL"."READ_NEWS" WHERE NEWS_ID = in_news_id;

    SELECT ::ROWCOUNT INTO out_result FROM DUMMY;

END;
