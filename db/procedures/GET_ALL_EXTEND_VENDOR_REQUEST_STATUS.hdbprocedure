PROCEDURE "CART_REQUEST_TOOL"."xscartrequesttool.db.procedures::GET_ALL_EXTEND_VENDOR_REQUEST_STATUS" (
	OUT out_result TABLE (status_id bigint, name nvarchar(255))
 ) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "CART_REQUEST_TOOL"
	READS SQL DATA AS
BEGIN
	out_result = SELECT EVRS.STATUS_ID, EVRS.NAME
		FROM "EXTEND_VENDOR_REQUEST_STATUS" EVRS
		WHERE EVRS.status_id > 1 AND EVRS.ENABLED = 1 AND EVRS.DELETED = 0;
END;
