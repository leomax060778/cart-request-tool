PROCEDURE "CART_REQUEST_TOOL"."xscartrequesttool.db.procedures::GET_ALL_ISSUE_TYPE" (
	OUT out_result TABLE (issue_type_id bigint, name nvarchar(255), additional_issue_type_information tinyint)
 ) 
	LANGUAGE SQLSCRIPT
	SQL SECURITY INVOKER 
	DEFAULT SCHEMA "CART_REQUEST_TOOL"
	READS SQL DATA AS
BEGIN
	out_result = SELECT issue_type_id, name, additional_issue_type_information
					FROM "ISSUE_TYPE"
						WHERE ENABLED = 1 AND DELETED = 0;
END;
